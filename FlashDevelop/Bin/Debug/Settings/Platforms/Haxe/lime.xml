<platform name="Lime" targets="html5,flash,windows,neko,android,tizen,blackberry,webos" debugger="flash" external="haxelib">
	<version value="1.0">
		<command name="display" value="lime display &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
		<command name="build" value="lime build &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
		<command name="run" value="lime run &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
		<command name="clean" value="lime clean &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
	</version>
</platform>
