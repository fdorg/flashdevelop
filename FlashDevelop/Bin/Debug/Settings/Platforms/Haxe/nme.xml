<platform name="Nme" targets="flash,windows,android" debugger="flash" external="haxelib">
	<version value="1.0">
		<command name="display" value="nme display &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
		<command name="build" value="nme build &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig) -Dfdb"/>
		<command name="run" value="nme run &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
		<command name="clean" value="nme clean &quot;$(OutputFile)&quot; $(TargetBuild) -$(BuildConfig)"/>
	</version>
</platform>
