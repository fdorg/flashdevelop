<!-- COLORING_START -->
<editor-style caret-fore="0x000000" caretline-back="0xececec" selection-fore="0xffffff" selection-back="0x3399ff" colorize-marker-back="true" margin-fore="0xffffff" margin-back="0xffffff" marker-back="0xbcbcbc" />
<use-styles>
	<style name="default" fore="0x000000" back="0xffffff" size="9" font="Consolas,Courier New,monospace" />
	<style name="tag" fore="0x800000" />
	<style name="class" fore="0x800000" />
	<style name="pseudoclass" fore="0x800000" />
	<style name="unknown_pseudoclass" fore="0xff0000" />
	<style name="operator" fore="0x800000" />
	<style name="identifier" />
	<style name="unknown_identifier" fore="0xff0000" />
	<style name="value" fore="0x800080" />
	<style name="comment" fore="0x808080" />
	<style name="id" fore="0x800000" />
	<style name="important" fore="0x800080" />
	<style name="directive" fore="0x800000" />
	<style name="doublestring" fore="0x0000ff" />
	<style name="singlestring" fore="0x0000ff" />
	<style name="identifier2" />
	<style name="attribute" />
	<style name="identifier3" />
	<style name="pseudoelement" />
	<style name="extended_identifier" />
	<style name="extended_pseudoclass" />
	<style name="extended_pseudoelement" />
	<style name="media" fore="0x800000" />
	<style name="variable" fore="0x00aa00" />
	<style name="gdefault" fore="0xefece9" />
	<style name="linenumber" fore="0x666666" />
	<style name="bracelight" fore="0x0000cc" back="0xcdcdff" bold="true" />
	<style name="bracebad" bold="true" />
	<style name="controlchar" fore="0xffffff" />
	<style name="indentguide" fore="0xc0c0c0" />
	<style name="lastpredefined" fore="0x666666" />
